! Set custom DPI
! Xft.dpi: 96

! XTerm
*vt100.allowBoldFonts: false
*vt100.cursorUnderLine: true
*vt100.cursorBlink: true
*vt100.faceName: Mono:bold:size=12
*vt100.saveLines: 60000
*vt100.utf8: true
*vt100.metaSendsEscape: true
*vt100.translations: #override \n\
	Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
	Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

! XTerm 
! special
*vt100.foreground:   #ffffff
*vt100.background:   #0c0c0d
*vt100.cursorColor:  #cecece

!! brogrammer:
